<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="powerchina.eportal"
    android:installLocation="internalOnly"
    android:versionCode="95"
    android:versionName="00.01.0014" >

    <application
        android:name="org.zywx.wbpalmstar.widgetone.WidgetOneApplication"
        android:allowBackup="false"
        android:allowClearUserData="false"
        android:hardwareAccelerated="false"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:vmSafeMode="true" >
        <activity
            android:name="org.zywx.wbpalmstar.engine.EBrowserActivity"
            android:alwaysRetainTaskState="true"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/browser_main_theme"
            android:windowSoftInputMode="stateHidden|adjustResize" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="appcanEPortal" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.zywx.wbpalmstar.engine.TempActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait"
            android:theme="@style/browser_loading_theme" />
        <activity
            android:name="org.zywx.wbpalmstar.engine.LoadingActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="standard"
            android:screenOrientation="portrait"
            android:theme="@style/browser_loading_theme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- push -->
        <receiver
            android:name="org.zywx.wbpalmstar.platform.push.PushRecieveMsgReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="org.zywx.push.receive" />
            </intent-filter>
        </receiver>

        <provider
            android:name="org.zywx.wbpalmstar.base.ACEContentProvider"
            android:authorities="powerchina.eportal.sp"
            android:exported="false" />

        <!-- 华为UC定制 -->
        <service android:name="com.huawei.service.EspaceService" />
        <service android:name="org.zywx.wbpalmstar.uc.UCAPIService" />

        <provider
            android:name="org.zywx.wbpalmstar.base.ACEContentProvider"
            android:authorities="powerchina.eportal.sp"
            android:exported="false" />

        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexappstoremgr.applistView.CategoryAppListActivity"
            android:configChanges="keyboardHidden|orientation"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/activity_anim_theme" />
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexappstoremgr.applistView.AppSearchActivity"
            android:configChanges="keyboardHidden|orientation"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/activity_anim_theme" />
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexcamera.CustomCamera"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.CAMERA_PLUGIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexcamera.ViewCamera.SecondActivity"
            android:label="@string/plugin_camera_title_activity_second" >
        </activity>
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexfilemgr.FilexplorerActivity"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@style/Style_platform_no_title_and_background" />
        <activity android:name="org.zywx.wbpalmstar.plugin.uexcontacts.ContactActivity" />
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexPicker.PickerActivity"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@android:style/Theme.NoTitleBar"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uextoast.ProgressMain"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent" >
        </activity>
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexaudio.AudioRecorderActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexaudio.PMusicPlayerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.zywx.wbpalmstar.plugin.uexaudio.PMusicFileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.zywx.wbpalmstar.plugin.ueximage.AlbumListActivity"
            android:theme="@style/plugin_uex_image_appTheme" >
        </activity>
        <activity
            android:name="org.zywx.wbpalmstar.plugin.ueximage.PictureGridActivity"
            android:theme="@style/plugin_uex_image_appTheme" >
        </activity>
        <activity
            android:name="org.zywx.wbpalmstar.plugin.ueximage.ImagePreviewActivity"
            android:theme="@style/plugin_uex_image_appTheme" >
        </activity>

        <service
            android:name="org.zywx.wbpalmstar.plugin.uexim.ChatService"
            android:enabled="true" />
        <service
            android:name="org.zywx.wbpalmstar.platform.push.PushService"
            android:enabled="true"
            android:process=":push" />

        <receiver
            android:name="org.zywx.wbpalmstar.plugin.uexappstoremgr.BroadcastReciver.EmmStartUpReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="org.zywx.emm.widgetupdate.event.action" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="org.zywx.wbpalmstar.platform.push.PushBroadCastReceiver"
            android:process=":push" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FIND_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <!-- 华为定制引擎新增权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.BROADCAST_STICKY" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <!-- camera permission -->
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="com.huawei.eSpaceMobileApp" />

    <permission
        android:name="com.huawei.eSpaceMobileApp"
        android:protectionLevel="normal" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.GET_TASKS" />

</manifest>